#1
a = int(input("길이(mm)를 입력하세요:"))
print(f"{a}mm --> {a*0.1}cm")
print(f"{a}mm --> {a*0.001}m")
print(f"{a}mm --> {a*0.03937}inch")
print(f"{a}mm --> {a*0.003281}ft")

#2
import myCircle

print(f"원 둘례:{myCircle.circumference()}")
print(f"원 둘례:{myCircle.area()}")


##모듈
import math

def circumference():
    a = 2*6*math.pi
    return a

def area():
    b = math.pi*6**2
    return b
